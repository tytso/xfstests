# FS QA Test No. 027
#
# Test out "xfsdump | xfsrestore"
#
. ./common/preamble
_begin_fstest dump ioctl auto quick

status=0	# success is the default!

# Override the default cleanup function.
_cleanup()
{
	_cleanup_dump
	cd /
	rm -f $tmp.*
}

# Import common functions.
. ./common/dump

# real QA test starts here
_supported_fs xfs

_create_dumpdir_fill
_do_dump_restore
_diff_compare_sub

# success, all done
exit
