# FS QA Test No. 046
#
# check on symlinks permissions
#
. ./common/preamble
_begin_fstest dump ioctl auto quick

# Override the default cleanup function.
_cleanup()
{
	_cleanup_dump
	cd /
	rm -f $tmp.*
}

# Import common functions.
. ./common/dump

# real QA test starts here
_supported_fs xfs

_create_dumpdir_symlinks
_do_dump_file
_do_restore_file
_ls_compare_sub

# success, all done
status=0
exit
