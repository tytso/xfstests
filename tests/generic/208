. ./common/preamble
_begin_fstest auto aio

# Import common functions.
. ./common/filter

# real QA test starts here

_supported_fs generic
_require_test

_run_aiodio aio-dio-invalidate-failure

_check_dmesg _filter_aiodio_dmesg

exit $status
